   <define name="TEI-att.datable.iso.attributes">
      <ref name="TEI-att.datable.iso.attribute.when-iso"/>
      <ref name="TEI-att.datable.iso.attribute.notBefore-iso"/>
      <ref name="TEI-att.datable.iso.attribute.notAfter-iso"/>
      <ref name="TEI-att.datable.iso.attribute.from-iso"/>
      <ref name="TEI-att.datable.iso.attribute.to-iso"/>
   </define>
   <define name="TEI-att.datable.iso.attribute.when-iso">
      <optional>
         <attribute name="when-iso">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">précise une date exacte pour l'évènement selon la forme normalisée ISO 8601, c'est-à-dire aaaa-mm-jj.</a:documentation>
            <choice>
               <data type="date"/>
               <data type="gYear"/>
               <data type="gMonth"/>
               <data type="gDay"/>
               <data type="gYearMonth"/>
               <data type="gMonthDay"/>
               <data type="time"/>
               <data type="dateTime"/>
               <data type="token">
                  <param name="pattern">[0-9.,DHMPRSTWYZ/:+\-]+</param>
               </data>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="TEI-att.datable.iso.attribute.notBefore-iso">
      <optional>
         <attribute name="notBefore-iso">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">précise la première date possible pour l'évènement selon la forme normalisée, c'est-à-dire aaaa-mm-jj.</a:documentation>
            <choice>
               <data type="date"/>
               <data type="gYear"/>
               <data type="gMonth"/>
               <data type="gDay"/>
               <data type="gYearMonth"/>
               <data type="gMonthDay"/>
               <data type="time"/>
               <data type="dateTime"/>
               <data type="token">
                  <param name="pattern">[0-9.,DHMPRSTWYZ/:+\-]+</param>
               </data>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="TEI-att.datable.iso.attribute.notAfter-iso">
      <optional>
         <attribute name="notAfter-iso">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">précise la dernière date possible pour l'évènement selon la forme normalisée, c'est-à-dire aaaa-mm-jj.</a:documentation>
            <choice>
               <data type="date"/>
               <data type="gYear"/>
               <data type="gMonth"/>
               <data type="gDay"/>
               <data type="gYearMonth"/>
               <data type="gMonthDay"/>
               <data type="time"/>
               <data type="dateTime"/>
               <data type="token">
                  <param name="pattern">[0-9.,DHMPRSTWYZ/:+\-]+</param>
               </data>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="TEI-att.datable.iso.attribute.from-iso">
      <optional>
         <attribute name="from-iso">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Indique le point de départ de la période sous une forme normalisée</a:documentation>
            <choice>
               <data type="date"/>
               <data type="gYear"/>
               <data type="gMonth"/>
               <data type="gDay"/>
               <data type="gYearMonth"/>
               <data type="gMonthDay"/>
               <data type="time"/>
               <data type="dateTime"/>
               <data type="token">
                  <param name="pattern">[0-9.,DHMPRSTWYZ/:+\-]+</param>
               </data>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="TEI-att.datable.iso.attribute.to-iso">
      <optional>
         <attribute name="to-iso">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Indique le point final de la période sous une forme normalisée</a:documentation>
            <choice>
               <data type="date"/>
               <data type="gYear"/>
               <data type="gMonth"/>
               <data type="gDay"/>
               <data type="gYearMonth"/>
               <data type="gMonthDay"/>
               <data type="time"/>
               <data type="dateTime"/>
               <data type="token">
                  <param name="pattern">[0-9.,DHMPRSTWYZ/:+\-]+</param>
               </data>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="TEI-orgName">
      <element name="orgName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(nom d'organisation) contient le nom d'une organisation.Projet Testaments de Poilus : peut être utilisé dans le corps de la transcription pour baliser un nom de régiment lorsque celui-ci fait l'objet d'une notice. L'élément orgName sert aussi dans les notices descriptives des régiments ; dans ce cas il aura un attribut full. L'élément orgName a obligatoirement un attribut type. [13.2.2. Organizational Names]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="TEI-lb"/>
               <ref name="TEI-pb"/>
               <ref name="TEI-choice"/>
               <ref name="TEI-gap"/>
               <ref name="TEI-unclear"/>
               <ref name="TEI-supplied"/>
               <ref name="TEI-damage"/>
               <ref name="TEI-add"/>
               <ref name="TEI-del"/>
               <ref name="TEI-subst"/>
               <ref name="TEI-app"/>
               <ref name="TEI-hi"/>
               <ref name="TEI-placeName"/>
               <ref name="TEI-ref"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-orgName-attrIfOrgNameInAuthorityRecord-constraint-rule-27">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:orgName[parent::tei:org]">
               <sch:assert test="@full">Indiquer si le nom est donné sous sa forme développée ou
                abrégée</sch:assert>
            </sch:rule>
         </pattern>
         <attribute name="type">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <choice>
               <value>regiment</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(nom de régiment) </a:documentation>
            </choice>
         </attribute>
         <optional>
            <attribute name="ref">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">L'attribut ref contient, le cas échéant, un renvoi vers la notice décrivant, au sein de la plate-forme de transcription, le régiment dont le nom a été balisé.</a:documentation>
               <data type="anyURI"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="full">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <text/>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="TEI-persName">
      <element name="persName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(nom de personne) contient un nom propre ou une expression nominale se référant à une personne, pouvant inclure tout ou partie de ses prénoms, noms de famille, titres honorifiques, noms ajoutés, etc.Projet Testaments de Poilus : peut être utilisé dans le corps de la transcriptin pour baliser un nom de personne lorsque la personne fait l'objet d'une notice. On l'utilise notamment dans la ou les mentions de signature du testament. Dans ce cas, il est recommandé de distinguer, dans la chaîne de caractères ainsi balisée, le nom de famille de la personne (afin que celui-ci, ultérieurement, puisse être mis en forme de manière spécifique dans l'édition de restitution). L'élément persName sert aussi pour baliser les métadonnées du testament et dans les notices sur les testateurs ; dans ce dernier cas, on pourra trouver dans cet élément les sous-éléments distinguant nom de famille, prénoms, particules, surnoms. [13.2.1. Personal Names]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="TEI-lb"/>
               <ref name="TEI-pb"/>
               <ref name="TEI-choice"/>
               <ref name="TEI-gap"/>
               <ref name="TEI-unclear"/>
               <ref name="TEI-supplied"/>
               <ref name="TEI-damage"/>
               <ref name="TEI-add"/>
               <ref name="TEI-del"/>
               <ref name="TEI-subst"/>
               <ref name="TEI-app"/>
               <ref name="TEI-hi"/>
               <ref name="TEI-surname"/>
               <ref name="TEI-forename"/>
               <ref name="TEI-nameLink"/>
               <ref name="TEI-addName"/>
               <ref name="TEI-ref"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-persName-contentIfPersNameInSigned-constraint-rule-28">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:persName[ancestor::tei:signed]">
               <sch:assert test="tei:surname">Penser à baliser le nom de famille de la
                personne</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-persName-contentIfPersNameInAuthorityRecord-constraint-rule-29">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:persName[parent::tei:person]">
               <sch:assert test="tei:surname and tei:forename">L'élément persName devrait contenir au moins
                  un élément surname et un élément forename</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-persName-contentIfPersNameInRespStmt-constraint-rule-30">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:persName[ancestor::tei:respStmt]">
               <sch:assert test="@xml:id or @corresp">Dans une mention de responsabilité, l'élément persName
                  devrait avoir un identifiant (attribut xml:id) ou pointer vers un autre élément
                  persName via l'attribut @corresp</sch:assert>
            </sch:rule>
         </pattern>
         <optional>
            <attribute name="ref">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">L'attribut ref contient, le cas échéant, un renvoi vers la notice décrivant, au sein de la plate-forme de transcription, la personne dont le nom a été balisé.</a:documentation>
               <data type="anyURI"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="full">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <text/>
            </attribute>
         </optional>
         <optional>
            <attribute name="xml:id">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <data type="ID"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="corresp">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <list>
                  <oneOrMore>
                     <data type="anyURI"/>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="TEI-surname">
      <element name="surname">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(nom de famille) contient un nom de famille (hérité) par opposition à un nom donné, nom de baptême ou surnom. [13.2.1. Personal Names]</a:documentation>
         <ref name="TEI-macro.phraseSeq"/>
         <ref name="TEI-att.global.attributes"/>
         <ref name="TEI-att.personal.attributes"/>
         <ref name="TEI-att.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="TEI-forename">
      <element name="forename">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(prénom) contient un prénom, qu'il soit donné ou un nom de baptême. [13.2.1. Personal Names]</a:documentation>
         <ref name="TEI-macro.phraseSeq"/>
         <ref name="TEI-att.global.attributes"/>
         <ref name="TEI-att.personal.attributes"/>
         <ref name="TEI-att.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="TEI-genName">
      <element name="genName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(qualificatif générationnel de nom) contient une composante de nom utilisée pour distinguer des noms, par ailleurs similaires, sur la base de l'âge ou de la génération des personnes concernées. [13.2.1. Personal Names]</a:documentation>
         <ref name="TEI-macro.phraseSeq"/>
         <ref name="TEI-att.global.attributes"/>
         <ref name="TEI-att.personal.attributes"/>
         <ref name="TEI-att.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="TEI-nameLink">
      <element name="nameLink">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(lien entre les composants d'un nom) contient une particule ou une expression exprimant un lien, utilisés dans un nom mais considérés comme n'en faisant pas partie, comme "van der" ou "de". [13.2.1. Personal Names]</a:documentation>
         <ref name="TEI-macro.phraseSeq"/>
         <ref name="TEI-att.global.attributes"/>
         <ref name="TEI-att.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="TEI-addName">
      <element name="addName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(nom additionnel) contient une composante de nom additionnelle, comme un surnom, une épithète, un alias ou toute autre expression descriptive utilisée dans un nom de personne. [13.2.1. Personal Names]</a:documentation>
         <ref name="TEI-macro.phraseSeq"/>
         <ref name="TEI-att.global.attributes"/>
         <ref name="TEI-att.personal.attributes"/>
         <ref name="TEI-att.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="TEI-roleName">
      <element name="roleName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(rôle) contient un composant du nom d'une personne, indiquant que celle-ci a un rôle ou une position particulière dans la société, comme un titre ou un rang officiel. [13.2.1. Personal Names]</a:documentation>
         <ref name="TEI-macro.phraseSeq"/>
         <ref name="TEI-att.global.attributes"/>
         <ref name="TEI-att.personal.attributes"/>
         <ref name="TEI-att.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="TEI-placeName">
      <element name="placeName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(nom de lieu) contient un nom de lieu absolu ou relatif.Peut être utilisé dans le corps de la transcription pour baliser un nom de lieu lorsque le lieu fait l'objet d'une notice. L'élément placeName sert aussi pour baliser les métadonnées du testament. Dans ces deux cas, l'attribut type permettra de dire de quel type de lieu il s'agit (lieu de rédaction du testament, lieu de naissance ou de décès du testateur, etc.). Enfin, cet élément est aussi utilisé dans les notices sur les lieux ; dans ce dernier cas, on trouvera dans cet élément les sous-éléments distinguant les composants du nom de lieu : nom de lieu habité, nom de lieu géographique, type générique de lieu (forêt, montagne...). Toujours dans le cas de la notice sur un lieu, le nom de lieu pourra le cas échéant avoir un attribut when-iso pour spécifier les dates d'utilisation de ce nom de lieu. [13.2.3. Place Names]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="TEI-lb"/>
               <ref name="TEI-pb"/>
               <ref name="TEI-choice"/>
               <ref name="TEI-gap"/>
               <ref name="TEI-unclear"/>
               <ref name="TEI-supplied"/>
               <ref name="TEI-damage"/>
               <ref name="TEI-add"/>
               <ref name="TEI-del"/>
               <ref name="TEI-subst"/>
               <ref name="TEI-app"/>
               <ref name="TEI-hi"/>
               <ref name="TEI-geogName"/>
               <ref name="TEI-geogFeat"/>
               <ref name="TEI-settlement"/>
               <ref name="TEI-ref"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-placeName-contentIfPlaceNameInAuthorityRecord-constraint-rule-31">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:placeName[ancestor::tei:place]">
               <sch:assert test="tei:settlement or tei:geogName or tei:geogFeat">Dans une notice sur un lieu,
                  l'élément placeName devrait contenir au moins un
                sous-élément</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TeiModel_for_FirstWorldWarWills-placeName-attrTypeIfPlaceNameInTranscriptionOrWillMetadata-constraint-rule-32">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:placeName[ancestor::tei:summary or ancestor::tei:div[@type='will'] or ancestor::tei:div[@type='envelope']]">
               <sch:assert test="@type">Dans les métadonnées sur le testament ou dans le corps
                  de la transcription, l'élément placeName devrait avoir un attribut
                  type</sch:assert>
            </sch:rule>
         </pattern>
         <optional>
            <attribute name="ref">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">L'attribut ref contient, le cas échéant, un renvoi vers la notice décrivant, au sein de la plate-forme de transcription, le lieu dont le nom a été balisé.</a:documentation>
               <data type="anyURI"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <choice>
                  <value>willPlace</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(lieu de rédaction du testament) </a:documentation>
                  <value>codicilPlace</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(lieu de rédaction du codicille) </a:documentation>
                  <value>willAuthorBirthPlace</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(lieu de naissance du testateur) </a:documentation>
                  <value>willAuthorDeathPlace</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(lieu de décès du testateur) </a:documentation>
                  <value>willAuthorResidence</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(domicile du testateur) </a:documentation>
                  <value>militaryEventPlace</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(lieu d'un événement militaire (bataille, offensive...)) </a:documentation>
                  <value>otherPlace</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(autre date) </a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="when-iso">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">la forme normalisée de la période pendant laquelle le nom de lieu a été utilisé (dans une notice sur un nom de lieu)</a:documentation>
               <choice>
                  <data type="date"/>
                  <data type="gYear"/>
                  <data type="gMonth"/>
                  <data type="gDay"/>
                  <data type="gYearMonth"/>
                  <data type="gMonthDay"/>
                  <data type="time"/>
                  <data type="dateTime"/>
                  <data type="token">
                     <param name="pattern">[0-9.,DHMPRSTWYZ/:+\-]+</param>
                  </data>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="TEI-bloc">
      <element name="bloc">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(bloc) contient le nom d'une unité géo-politique composée d'au moins deux états ou pays [13.2.3. Place Names]</a:documentation>
         <ref name="TEI-macro.phraseSeq"/>
         <ref name="TEI-att.global.attributes"/>
         <ref name="TEI-att.naming.attributes"/>
         <ref name="TEI-att.typed.attributes"/>
         <ref name="TEI-att.datable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="TEI-country">
      <element name="country">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(pays) contient le nom d'une unité géo-politique, comme une nation, un pays, une colonie ou une communauté, plus grande ou administrativement supérieure à une région et plus petite qu'un bloc. [13.2.3. Place Names]</a:documentation>
         <ref name="TEI-macro.phraseSeq"/>
         <ref name="TEI-att.global.attributes"/>
         <ref name="TEI-att.naming.attributes"/>
         <ref name="TEI-att.typed.attributes"/>
         <ref name="TEI-att.datable.attributes"/>
         <empty/>
      </element>
   </define>
