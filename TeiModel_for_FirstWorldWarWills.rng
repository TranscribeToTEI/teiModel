      <ref name="TEI-att.handFeatures.attribute.script"/>
      <ref name="TEI-att.handFeatures.attribute.scriptRef"/>
      <ref name="TEI-att.handFeatures.attribute.medium"/>
      <ref name="TEI-att.handFeatures.attribute.scope"/>
